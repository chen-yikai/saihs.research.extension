{
  "manifest_version": 3,
  "name": "Summary Reader",
  "description": "A tool can help you read summary webs page content that summarized by AI",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png"
  }
}
